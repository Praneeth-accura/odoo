<?xml version="1.0" encoding="utf-8"?>
<odoo>
    	 <record id="stock_inventory_line_inherit_2" model="ir.ui.view">
            <field name="name">stock.inventory.form.inherit</field>
            <field name="inherit_id" ref="stock.view_inventory_form"/>
            <field name="model">stock.inventory</field>
            <field name="arch" type="xml">
                <field name="product_qty" position="after">
                    <field name="different_qty" />
                    <field name="standard_prices" invisible="1"/>
                    <field name="cost_prices" invisible="1"/>
                    <field name="state" invisible="1"/>
                </field>
            </field>
        </record>


     <record id="stock_inventory_line_inherit" model="ir.ui.view">
            <field name="name">stock.inventory.line.inherit</field>
            <field name="inherit_id" ref="stock.stock_inventory_line_tree2"/>
            <field name="model">stock.inventory.line</field>
            <field name="arch" type="xml">
                <field name="product_qty" position="after">
                    <field name="different_qty" />
                </field>
            </field>
        </record>

</odoo>