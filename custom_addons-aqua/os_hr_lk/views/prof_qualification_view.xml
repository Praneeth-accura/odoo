<?xml version="1.0" encoding="utf-8"?>
<odoo>

	 <!-- Qualification View -->
     <record id="prof_qualification_view_form" model="ir.ui.view">
     	<field name="name">hr.qualification.form</field>
     	<field name="model">prof.qualification</field>
     	<field name="arch" type="xml">
        	<form string="Qualification">
            	<group>
                	<field name="name" required="1"/>
             	</group>
         	</form>
     	</field>
     </record>
     
     <record id="prof_qualification_view_tree" model="ir.ui.view">
     	<field name="name">hr.qualification.tree</field>
     	<field name="model">prof.qualification</field>
     	<field name="arch" type="xml">
        	<tree string="Qualification">
            	<field name="name"/>
         	</tree>
     	</field>
     </record>
        
     <record id="prof_qualification_view" model="ir.actions.act_window">
     	<field name="name">Qualification</field>
   	 	<field name="res_model">prof.qualification</field>
   	 	<field name="view_type">form</field>
   	 	<field name="view_mode">tree,form</field>
   	 	<field name="help" type="html">
     		<p class="oe_view_nocontent_create">
       			Click to create a religion.
     		</p>
   	 	</field>
     </record>
     
     <menuitem id="menu_qualification" action="prof_qualification_view" name="Qualifications" 
        parent="hr.menu_human_resources_configuration" sequence="88"/>
        
</odoo>