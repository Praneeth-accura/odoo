        <template id="issue_return_report_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="doc">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <br/><br/>
                            <div class="row">
                                <div class="col-xs-2">
                                    <strong>Issue Number :</strong>
                                </div>
                                <div>
                                    <span t-field="doc.name"/>
                                </div>

                                <div class="col-xs-2">
                                    <strong>Date :</strong>
                                </div>
                                <div>
                                    <span t-field="doc.date"/>
                                </div>
                                <div class="col-xs-2">
                                    <strong>Location :</strong>
                                </div>
                                <div>
                                    <span t-field="doc.location_id.name"/>
                                </div>


                                <div class="col-xs-2">
                                    <strong>Type :</strong>
                                </div>
                                <div>
                                    <span t-field="doc.type"/>
                                </div>
                            </div>
                            <br/>
                            <!--<div class="row">-->

                            <!--</div>-->


                            <!--<p>-->
                            <!--<strong>Journal Items</strong>-->
                            <!--</p>-->

                            <table class="table table-condensed table-striped">
                                <tr>
                                    <th>Charge Type</th>
                                    <th>Product</th>
                                    <th>On Hand Qty</th>
                                    <th colspan="1">Quantity</th>
                                    <th>Unit of Measure</th>
                                    <th>Analytic Account</th>
                                    <th>Analytic Tags</th>
                                    <th>Memo</th>
                                    <!--<th class="text-right">Debit</th>-->
                                    <!--<th class="text-right">Credit</th>-->
                                </tr>

                                <tr t-foreach="doc.itemlines" t-as="line">
                                    <td>
                                        <span t-field="line.charge_type"/>
                                    </td>
                                    <td>
                                        <span t-field="line.product_id"/>
                                    </td>
                                    <td>
                                        <span t-field="line.onhand_quantity"/>
                                    </td>
                                    <td>
                                        <span t-field="line.scrap_quantity"/>
                                    </td>

                                    <td>
                                        <span t-field="line.product_uom_id"/>
                                    </td>

                                    <td>
                                        <span t-field="line.analytic_account_id"/>
                                    </td>


                                    <td>
                                        <t t-foreach="line.analytic_tag_ids" t-as="l">
                                            <span t-field="l.name"/>
                                            <br/>
                                        </t>
                                    </td>


                                    <td class="text-right">
                                        <span t-field="line.memo_text"/>
                                    </td>
                                    <!--<td class="text-right">-->
                                    <!--<span t-field="line.credit"/>-->
                                    <!--</td>-->
                                </tr>

                                <!--<tr>-->
                                <!--<td class="text-right" colspan="7">Total</td>-->
                                <!--<td class="text-right">-->
                                <!--<span t-field="doc.amount"/>-->
                                <!--</td>-->
                                <!--<td class="text-right">-->
                                <!--<span t-field="doc.amount"/>-->
                                <!--</td>-->
                                <!--</tr>-->

                            </table>

                            <!--<p>-->
                            <!--<span t-field="doc.narration"/>-->
                            <!--</p>-->


                            <div style="position:absolute;top:850px; page-break-inside: avoid;">

                                <table class="table">
                                    <tr>
                                        <td>Created By</td>
                                        <td>Checked By</td>
                                        <td>Approved By</td>
                                        <td>Received By</td>
                                    </tr>
                                    <tr>
                                        <td style="padding-top:2cm">
                                            <span t-field="doc.create_uid"/>
                                        </td>
                                        <td style="padding-top:2cm">
                                            ....................
                                        </td>
                                        <td style="padding-top:2cm">
                                            ....................
                                        </td>
                                        <td style="padding-top:2cm">
                                            ....................
                                        </td>

                                    </tr>
                                </table>

                            </div>


                        </div>


                    </t>
                </t>
            </t>
        </template>

