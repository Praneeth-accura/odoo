<odoo>
  <data>

      <record id="stock_move_issuance1" model="ir.ui.view">
            <field name="name">stock.move.form</field>
            <field name="model">stock.move</field>
            <field name="inherit_id" ref="stock.view_move_form"/>
            <field name="arch" type="xml">
                <!--<xpath expr="//group[@name='origin_grp']" position="inside">-->
                    <!--<field name="purchase_line_id"/>-->
                <!--</xpath>-->
                <field name="reference" position="after">
                    <field name="issuance_date" invisible="1"/>
                    <field name="is_issuance" invisible="1"/>
                </field>
            </field>
        </record>





      <record model="ir.ui.view" id="charge_type">
          <field name="name">chargetype.form</field>
          <field name="model">charge.type</field>
          <field name="arch" type="xml">
              <form string="Charge type form">
                  <group>
                      <field name="name"/>
                      <field name="account_control_ids"/>
                  </group>
              </form>
          </field>
      </record>

      <record id="view_odoo_purchase_order_form_inherit_shipment" model="ir.ui.view">
        <field name="name">Purchase inherited form</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">

            <xpath expr="//button[@name='action_view_invoice']" position="replace">
                <button type="object" name="action_view_invoice" class="oe_stat_button" icon="fa-pencil-square-o" attrs="{'invisible':['|',('state', 'in', ('draft','sent','to approve')),('invoice_status','=','no'),('invoice_ids','=',[])]}">
                    <field name="invoice_count" widget="statinfo" string="Vendor Bills"/>
                    <field name="invoice_ids" invisible="1"/>
                </button>
            </xpath>
            <!--<xpath expr="//field[@name='partner_id']" position="after">-->
                <!--<field name="is_shipment_done" />-->
            <!--</xpath>-->
       </field>
   </record>

      <record model="ir.actions.act_window" id="charge_type_action">
          <field name="name">ChargeType</field>
          <field name="res_model">charge.type</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
              <p class="oe_view_nocontent_create">Add your Charge type</p>
          </field>
      </record>

      <menuitem id="Charge_type" name="Charge Type" parent="stock.menu_stock_inventory_control" action="charge_type_action"/>

      <record model="ir.ui.view" id="itemissuance_tree">
          <field name="name">item.issue.return.tree</field>
          <field name="model">item.issue.return</field>
          <field name="arch" type="xml">
              <tree string="Item Issuance and Return">
                  <field name="name"/>
                  <field name="location_id"/>
                  <field name="type"/>
                  <field name="date"/>
                  <field name="state"/>
              </tree>
          </field>
      </record>
      <record model="ir.ui.view" id="itemissuance">
          <field name="name">item.issue.return.form</field>
          <field name="model">item.issue.return</field>
          <field name="arch" type="xml">
              <form string="Item Issuance and Return">
                  <header>
                      <button name="action_confirm" string="Confirm" states="draft" type="object"/>
                      <field name="state" widget="statusbar" statusbar_visible="draft,confirm"/>
                  </header>
                  <sheet>

                      <group>
                          <group>
                              <field name="name"/>
                              <field name="location_id" attrs="{'readonly': [('state', '=', 'confirm')]}"/>
                              <field name="type" attrs="{'readonly': [('state', '=', 'confirm')]}"/>
                              <field name="scrap_location_id" invisible="1"/>
                          </group>
                          <group>

                              <field name="date" attrs="{'readonly': [('state', '=', 'confirm')]}"/>
                              <field name="legacy_ref_no"/>

                          </group>
                      </group>
                      <notebook>
                          <page string="Item Lines">
                              <field name="itemlines" widget="one2many_list">
                                  <tree editable="bottom">
                                      <field name="charge_type"/>
                                      <!--<field name="account_id"/>-->
                                      <field name="item_issuance_return_Id" invisible="1" />
                                      <!--<field name="location_id"/>-->
                                      <field name="product_id"/>
                                      <field name="onhand_quantity"/>
                                      <field name="scrap_quantity"/>
                                      <field name="product_uom_id"/>
                                      <field name="analytic_account_id"/>
                                      <field name="analytic_tag_ids" widget="many2many_tags"/>

                                      <field name="legacy_ref_no"/>
                                      <field name="memo_text"/>
                                  </tree>
                              </field>
                          </page>
                      </notebook>
                  </sheet>
              </form>
          </field>
      </record>

      <record model="ir.actions.act_window" id="item_issuance_action">
          <field name="name">Item Issuance and Return</field>
          <field name="res_model">item.issue.return</field>
          <field name="view_type">form</field>
          <field name="view_mode">tree,form</field>
          <field name="help" type="html">
              <p class="oe_view_nocontent_create">Add your Issuance Item</p>
          </field>
      </record>

      <menuitem id="form_menu" name="Item Issuance and Return" parent="stock.menu_stock_inventory_control" action="item_issuance_action"/>


      <record id="cost_field_in_inventory" model="ir.ui.view">
            <field name="name">stock.inventory.form.inherit</field>
            <field name="model">stock.inventory</field>
            <field name="inherit_id" ref="stock.view_inventory_form"/>
            <field name="arch" type="xml">
                <xpath expr="//tree//field[@name='location_id']" position="after">
                    <!-- <field name="cost"/> -->
                </xpath>
            </field>
        </record>


  </data>
</odoo>
